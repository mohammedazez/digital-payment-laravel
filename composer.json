{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "albertcht/invisible-recaptcha": "^1.9",
        "barryvdh/laravel-dompdf": "^0.8.1",
        "facebook/graph-sdk": "^5.6",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "kevupton/laravel-coinpayments": "^0.2.9",
        "khill/lavacharts": "3.1.*",
        "klaravel/ntrust": "1.1.*",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.0",
        "laravel/socialite": "^3.0",
        "laravelcollective/html": "^5.5.0",
        "mike42/escpos-php": "^2.0",
        "milon/barcode": "^5.3",
        "nesbot/carbon": "^2.0",
        "queueworker/sansdaemon": "^1.1",
        "r3k4/freesms4us": "dev-master",
        "rap2hpoutre/laravel-log-viewer": "^1.1",
        "socialiteproviders/instagram": "^3.0",
        "spatie/laravel-permission": "^3.13",
        "trijayadigital/cekmutasi-laravel": "dev-master",
        "webpatser/laravel-uuid": "2.*",
        "yajra/laravel-datatables-oracle": "^9.7",
        "zerosdev/laravel-captcha": "dev-master"
    },
    "require-dev": {
        "composer/composer": "^1.9",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tripay\\Pulsa\\": "packages/tripay/pulsa/src",
            "Appnotif\\Notif\\": "packages/appnotif/notif/src",
            "ZerosDev\\ZerosSMS\\": "packages/zerosdev/zerossms/src",
             "Serverpulsa\\Pulsa\\DigiFlazz\\": "packages/serverpulsa/pulsa/digiflazz/src"
          
        },
        "files": [
            "app/Http/Helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        
        "platform": {

        "php": "7.3"

    }
    }
}
